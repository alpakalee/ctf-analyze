{
  "title": "CTF 학습 노트",
  "description": "CTF 문제 풀이 및 해킹 기법 정리",
  "author": "alpakalee",
  "language": "ko",
  "gitbook": ">=3.2.3",
  "plugins": [
    "search-pro",
    "code",
    "copy-code-button",
    "theme-comscore",
    "expandable-chapters",
    "splitter",
    "back-to-top-button",
    "-lunr",
    "-search",
    "-sharing"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": ["CTF", "XSS", "SQL", "CSRF", "RCE", "LFI", "RFI"]
    },
    "code": {
      "copyButtons": true
    }
  },
  "links": {
    "sidebar": {
      "GitHub Repository": "https://github.com/alpakalee/ctf-analyze"
    }
  },
  "styles": {
    "website": "styles/website.css"
  }
}
